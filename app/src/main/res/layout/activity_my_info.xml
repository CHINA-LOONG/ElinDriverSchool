<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@color/line_color_gray"
    android:orientation="vertical">

    <include android:id="@+id/ll_my_info_title"
        layout="@layout/custom_go_back"/>
    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:background="@color/white"
        android:orientation="vertical"
        android:layout_marginTop="@dimen/marginTop"
        >
        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:padding="@dimen/marginLeft"
            android:orientation="horizontal"
            android:gravity="center_vertical"
            >
            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="姓        名："
                android:textColor="@color/text_color_light_dark"
                android:textSize="@dimen/text_normal_size"
                />
            <TextView
                android:id="@+id/tv_my_info_name"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:text="李四"
                android:textColor="@color/text_color_nomal"
                android:textSize="@dimen/text_normal_size"
                android:layout_marginLeft="@dimen/marginTop"
                android:gravity="end"
                />
        </LinearLayout>
        <View
            android:layout_width="match_parent"
            android:layout_height="@dimen/line_height"
            android:background="@color/line_color_gray"
            android:layout_marginLeft="@dimen/marginTop"
            android:layout_marginRight="@dimen/marginTop"/>
        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:padding="@dimen/marginLeft"
            android:orientation="horizontal"
            android:gravity="center_vertical"
            >
            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="手  机  号："
                android:textColor="@color/text_color_light_dark"
                android:textSize="@dimen/text_normal_size"
                />
            <TextView
                android:id="@+id/tv_my_info_phone"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:text="1345678998"
                android:gravity="end"
                android:textColor="@color/text_color_nomal"
                android:textSize="@dimen/text_normal_size"
                android:layout_marginLeft="@dimen/marginTop"
                />
        </LinearLayout>
        <View
            android:layout_width="match_parent"
            android:layout_height="@dimen/line_height"
            android:background="@color/line_color_gray"
            android:layout_marginLeft="@dimen/marginTop"
            android:layout_marginRight="@dimen/marginTop"/>
        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:padding="@dimen/marginLeft"
            android:orientation="horizontal"
            android:gravity="center_vertical"
            >
            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="身  份  证："
                android:textColor="@color/text_color_light_dark"
                android:textSize="@dimen/text_normal_size"
                />
            <TextView
                android:id="@+id/tv_my_info_id_num"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:text="948374r89374"
                android:gravity="end"
                android:textColor="@color/text_color_nomal"
                android:textSize="@dimen/text_normal_size"
                android:layout_marginLeft="@dimen/marginTop"
                />
        </LinearLayout>
    </LinearLayout>
