<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@color/line_color_gray"
    android:orientation="vertical">

    <include android:id="@+id/ll_upload_stu_grade_title"
        layout="@layout/custom_go_back"/>
    <com.elin.elindriverschool.view.SlidingTabLayout
        android:id="@+id/slid_upload_stu_grade"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:background="@android:color/white"
        ></com.elin.elindriverschool.view.SlidingTabLayout>
    <android.support.v4.view.ViewPager
        android:id="@+id/vp_upload_stu_grade"
        android:layout_width="match_parent"
        android:layout_height="0dp"
        android:layout_weight="1"
        ></android.support.v4.view.ViewPager>


</LinearLayout>